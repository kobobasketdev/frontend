{
  "name": "kobofrontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "jest",
    "test:unit": "jest \"--testMatch=**/*.unit.spec.[jt]s?(x)\"",
    "tdd": "npm test -- --watch",
    "tdd:unit": "npm run test:unit -- --watch",
    "lint:fix": "npm run lint -- --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate-route": "tsr generate",
    "watch-route": "tsr watch"
  },
  "imports": {
    "#hooks/mutations/*": {
      "storybook": "./src/hooks/mutations/*.mock.ts",
      "default": "./src/hooks/mutations/*.ts"
    },
    "#hooks/query/*": {
      "storybook": "./src/hooks/queries/*.mock.ts",
      "default": "./src/hooks/queries/*.ts"
    },
    "#*": [
      "./src/*",
      "./src/*.ts",
      "./src/*.tsx"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.1",
    "@mui/material": "^6.4.1",
    "@reduxjs/toolkit": "^2.5.0",
    "@tanstack/react-router": "^1.97.20",
    "dayjs": "^1.11.13",
    "html-react-parser": "^5.2.2",
    "lodash": "^4.17.21",
    "millify": "^6.1.0",
    "notistack": "^3.0.2",
    "pluralize": "^8.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@eslint/js": "^9.19.0",
    "@storybook/addon-essentials": "^8.5.1",
    "@storybook/addon-interactions": "^8.5.1",
    "@storybook/addon-onboarding": "^8.5.1",
    "@storybook/addon-viewport": "^8.5.1",
    "@storybook/blocks": "^8.5.1",
    "@storybook/react": "^8.5.1",
    "@storybook/react-vite": "^8.5.1",
    "@storybook/test": "^8.5.1",
    "@stylistic/eslint-plugin": "^3.0.0",
    "@tanstack/eslint-plugin-query": "^5.64.2",
    "@tanstack/router-cli": "^1.97.20",
    "@tanstack/router-devtools": "^1.97.20",
    "@tanstack/router-plugin": "^1.97.20",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.14",
    "@types/pluralize": "^0.0.33",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "eslint-plugin-storybook": "^0.11.2",
    "globals": "^15.14.0",
    "jest-environment-jsdom": "^29.7.0",
    "storybook": "^8.5.1",
    "ts-jest": "^29.2.5",
    "ts-jest-mock-import-meta": "^1.2.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.21.0",
    "vite": "^6.0.11"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
